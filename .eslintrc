{
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["jest"],
  "rules": {
    "brace-style": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ],
    "func-names": "off",
    "global-require": "off",
    "import/no-extraneous-dependencies": "off",
    "comma-dangle": "off",
    "no-use-before-define": "off"
  },
  "env": {
    "node": true,
    "jest": true
  }
}
